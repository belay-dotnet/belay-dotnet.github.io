{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "belay-source/src/Belay.Attributes/Belay.Attributes.csproj",
            "belay-source/src/Belay.Core/Belay.Core.csproj",
            "belay-source/src/Belay.Extensions/Belay.Extensions.csproj",
            "belay-source/src/Belay.Sync/Belay.Sync.csproj"
          ],
          "exclude": [
            "**/bin/**",
            "**/obj/**"
          ]
        }
      ],
      "dest": "api/metadata",
      "includePrivateMembers": false,
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "noRestore": false,
      "namespaceLayout": "flattened",
      "memberLayout": "samePage",
      "allowCompilationErrors": true,
      "properties": {
        "TargetFramework": "net8.0",
        "Configurations": "Release",
        "TreatWarningsAsErrors": "false",
        "WarningsAsErrors": "",
        "WarningsNotAsErrors": "",
        "NoWarn": "1570;1571;1572;1573;1574;1580;1581;1584;1587;1589;1590;1591;1572;CS1591;SA1500;SA1201;SA1203;SA1117;SA1611;SA1651;SA1500;SA1649",
        "RunAnalyzersDuringBuild": "false",
        "RunStyleCopAnalyzers": "false",
        "EnableNETAnalyzers": "false"
      }
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/metadata/*.yml",
          "api/metadata/**/*.yml"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "api/generated",
    "template": [
      "default",
      "modern"
    ],
    "postProcessors": ["ExtractSearchIndex"],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false,
    "globalMetadata": {
      "_appTitle": "Belay.NET API Reference",
      "_appName": "Belay.NET",
      "_enableSearch": true,
      "_gitContribute": {
        "repo": "https://github.com/belay-dotnet/Belay.NET",
        "branch": "main"
      }
    }
  }
}